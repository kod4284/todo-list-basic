{
  "extends": ["react-app", "airbnb-typescript"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./src"]],
        "extensions": [".js", ".ts", ".tsx", ".css", ".scss"]
      }
    }
  },
  
  "parserOptions": {
    "project": "./tsconfig.json"
    
  },
  "rules": {
    "linebreak-style": 0
  }
}
